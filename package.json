{
  "name": "react-redux-hmr-ssr",
  "version": "1.0.0",
  "description": "isomorphic react-redux server-side-rendering hot-reload boilerplate",
  "main": "",
  "scripts": {
    "test": "test",
    "lint:eslint": "eslint --ext=.js --ext=.jsx source/client",
    "start": "concurrently --kill-others \"npm run start:proxy\" \"npm run start:dev\"",
    "start:dev": "webpack-dev-server --open",
    "start:proxy": "nodemon source/server",
    "start:prod": "npm run build:dist && cross-env NODE_ENV=production node dist/server",
    "build:dist": "npm run build:assets && rm -rf dist/ && babel source/ -d dist/",
    "build:assets": "cross-env NODE_ENV=production webpack"
  },
  "pre-commit": [
    "lint:eslint"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
